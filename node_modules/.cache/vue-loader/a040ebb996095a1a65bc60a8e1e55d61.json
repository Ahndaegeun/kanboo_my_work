\" :to=\"item.path\">{{item.name}}</router-link>\n        </li>\n      </ul>\n    </li>\n  </ul>  \n\n</template>\n\n<script>\nimport MenuContent from '../../../assets/MenuList.json'\nexport default {\n  name: 'menu',\n  data() {\n    return {\n      list: MenuContent,\n      menuList: {\n        \"left\": {},\n        \"right\": []\n      }\n    }\n  },\n  watch: {\n    '$route' (to) {\n      const path = to.path\n      const obj = {\n        \"left\": {},\n        \"right\": []\n      }\n      switch(path) {\n        case '/':\n          obj.left = this.MenuContent.noAccess.left\n          obj.right = this.MenuContent.noAccess.home\n          break;\n        case '/community':\n          obj.left = this.MenuContent.noAccess.left\n          obj.right = this.MenuContent.noAccess.community\n          break;\n        case '/signin':\n          obj.left = this.MenuContent.noAccess.left\n          obj.right = this.MenuContent.noAccess.sign\n          break;\n        case '/demo':\n          obj.left = this.MenuContent.noAccess.left\n          obj.right = this.MenuContent.noAccess.home\n          break;\n      }\n      this.menuList = obj\n    }\n  }\n}\n</script>\n\n<style scoped>\n.nav-container {\n  height: 100%;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 30px;\n}\n\nimg {\n  width: 100px;\n}\n\nbutton {\n  color: #fff;\n}\n\n.left-menu {\n  display: flex;\n}\n\n.route-menu {\n  padding: 0 10px;\n  color: #999;\n  transition: color .2s ease-in;\n}\n\n.route-menu:hover {\n  color: #fff;\n}\n</style>"]}]}