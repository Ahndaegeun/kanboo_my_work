right\": []\n      }\n\n      if(path.includes('pdtail')) {\n        obj.left = this.list.projectDetail.left\n        obj.right = this.list.projectDetail.right\n      } else {\n        obj.left = this.list.noAccess.left\n      }\n\n      switch(path) {\n        case '/':\n          obj.right = this.list.noAccess.home\n          break\n        case '/community':\n          obj.right = this.list.noAccess.community\n          break\n        case '/signin':\n          obj.right = this.list.noAccess.sign\n          break\n        case '/demo':\n          obj.right = this.list.noAccess.home\n          break\n        case '/projects':\n          obj.right = this.list.access.right\n          break\n      }\n      this.menuList = obj\n    }\n  }\n}\n</script>\n\n<style scoped>\n.nav-container {\n  height: 100%;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 30px;\n}\n\nimg {\n  width: 100px;\n}\n\nbutton {\n  color: #fff;\n}\n\n.left-menu {\n  display: flex;\n}\n\n.route-menu {\n  padding: 0 10px;\n  color: #999;\n  transition: color .2s ease-in;\n}\n\n.route-menu:hover {\n  color: #fff;\n}\n\n.check {\n  color: #fff;\n}\n</style>"]}]}